r\n    height: 90vh;\r\n\tdisplay: flex;\r\n\tjustify-content: center;\r\n    align-items: center;\r\n}\r\n\r\n.card-container{\r\n\twidth: 500px;\r\n    height: 500px;\r\n}\r\n\r\n.card{\r\n    width: 100%;\r\n    height: 100%;\r\n    transition: transform 1s ease-in-out;\r\n    transform-style: preserve-3d;\r\n}\r\n\r\n.card-container:hover .card{\r\n    transform: rotateY(180deg);\r\n}\r\n\r\n.card figure{\r\n    width: 100%;\r\n    height: 100%;\r\n    border-radius: 50%;\r\n    position: absolute;\r\n    backface-visibility: hidden;\r\n    margin: 0;\r\n}\r\n\r\n.card .front{\r\n    img{\r\n        border-radius: 50%;\r\n    }\r\n}\r\n\r\n.card .back{\r\n    background-color: #000000;\r\n    color: #ffffff;\r\n    transform: rotateY(180deg);\r\n    text-align: center;\r\n    align-items: center;\r\n    justify-content: center;\r\n    display: flex;\r\n    flex-direction: column;\r\n    h1{\r\n        font-size: 50px;\r\n    }\r\n   \r\n}\r\n\r\n/* SOCIAL ICONS */\r\n.social-buttons{\r\n    list-style: none;\r\n    margin-top: 30%;\r\n    display: flex;\r\n    li{\r\n        padding: 10px 30px;\r\n    }\r\n}\r\n\r\n.twitter-colour{\r\n    background-image: url(../img/icons/twitter.png);\r\n    border-radius: 100%;\r\n}\r\n\r\n.twitter-colour:hover{\r\n    background-image: url(../img/icons/twitter_white.png);\r\n    background-color: #38A1F3;\r\n}\r\n\r\n.linkedIn-colour{\r\n    background-image: url(../img/icons/linkedIn.png);\r\n    border-radius: 100%;\r\n}\r\n\r\n.linkedIn-colour:hover{\r\n    background-image: url(../img/icons/linkedIn_white.png);\r\n    background-color: #0077B5;\r\n}\r\n\r\n.gmail-colour{\r\n    background-image: url(../img/icons/gmail.png);\r\n    border-radius: 100%;\r\n}\r\n\r\n.gmail-colour:hover{\r\n    background-image: url(../img/icons/gmail_hover.png);\r\n    background-color: #ffffff;\r\n}\r\n\r\n/* CARD CONTAINER (PROJECT) */\r\n// .container-proj{\r\n// \twidth: 100%;\r\n// \theight: 90vh;\r\n// \tdisplay: flex;\r\n// \tjustify-content: space-evenly;\r\n// \talign-items: center;\r\n// }\r\n\r\n// .card-container-proj{\r\n// \twidth: 375px;\r\n//     height: 500px;\r\n// }\r\n\r\n// .card-proj{\r\n//     width: 100%;\r\n// \theight: 100%;\r\n//     transform: rotateX(75deg);\r\n//     transition: transform 1s ease-in-out;\r\n// \ttransform-style: preserve-3d;\r\n// \tbackground-color: #000000;\r\n// }\r\n\r\n// .card-container-proj:hover .card-proj{\r\n//     transform: rotateX(0deg);\r\n// }\r\n\r\n// .card-text {\r\n// \twidth: 90%;\r\n// \tposition: absolute;\r\n// \tline-height: 30px;\r\n// \ttop: 70%;\r\n// \tleft: 50%;\r\n// \ttransform: translate(-50%, -50%);\r\n//     color: #ffffff;\r\n//     .button{\r\n//         margin-top: 20%;\r\n// \t    margin-left: 35%;\r\n//     }\r\n//   }\r\n  \r\n// .button {\r\n//     background-color: #ffffff;\r\n//     border: none;\r\n//     color: #000000;\r\n//     padding: 20px 20px;\r\n//     text-align: center;\r\n//     text-decoration: none;\r\n//     display: inline-block;\r\n//     border-radius: 100%;\r\n//   }\r\n\r\n/* PROJECTS */\r\n.container-projects {\r\n\twidth: 100%;\r\n    height: 90vh;\r\n    display: flex;\r\n\tjustify-content: center;\r\n    align-items: center;\r\n}\r\n\r\n.image-divide{\r\n    width: 80%;\r\n    height: 45vh;\r\n    overflow-y: scroll;\r\n    overflow-x: hidden;\r\n    scrollbar-color: grey white;\r\n    scrollbar-width: thin;\r\n}\r\n\r\n.responsive{\r\n    margin: 18px 0 0 18px;\r\n}\r\n\r\n/* MEDIA */\r\n@media screen and (max-width: 1199px){\r\n    .container, \r\n    img{\r\n\t\twidth: 100%;\r\n\t\theight: 100%;\r\n    }\r\n}\r\n  \r\n/* MEDIA */\r\n@media screen and (max-width: 255px){\r\n    .container-projects, \r\n    .responsive{\r\n\t\twidth: 100%;\r\n\t\theight: auto;\r\n    }\r\n}"},"lineCount":null}},"error":null,"hash":"b70e027fc10ab6cee44a2faa78490b38","cacheData":{"env":{}}}